{
  "name": "huge-puppy-2",
  "version": "1.0.0",
  "description": "huge puppy's personal site",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "dev:tailwind": "tailwindcss -i src/assets/css/tailwind.css -o _site/assets/css/tailwind.css --watch --postcss",
    "dev:11ty": "eleventy --serve",
    "build": "NODE_ENV=production eleventy && tailwindcss -i public/css/tailwind.css -o _site/public/tailwind.css --postcss"
  },
  "keywords": [
    "developer",
    "personal site",
    "huge puppy",
    "jared lambert"
  ],
  "author": "huge puppy",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6"
  }
}
